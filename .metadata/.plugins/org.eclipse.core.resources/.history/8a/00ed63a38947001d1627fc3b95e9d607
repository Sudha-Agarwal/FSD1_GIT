class Class1{
	
}

class Class2{
	
}

public class MainClass {
	
	
	//method definition
	
	//num1 and num2 are called as parameters	
	/* 
	 * 
	 * */
	static int sum(int num1, int num2){
		int result = num1 + num2;
		return result;
	}
	
	public static void main(String[] args) {
		
		int return_val = sum(2,3);
		System.out.println(return_val);

	}

}
